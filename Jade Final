    "expend['Total Expenditures'] = total['Total Expenditures']\n",
    "expend['People per HH'] = num_people['People per HH']\n",
    "expend['Expenditures per capita'] = expend['Total Expenditures'] / expend['People per HH']\n",
    "expend"
